{
  "name": "local-proxy",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "start": "nodemon index.js --site=intranet --port=8081",
    "start-80": "nodemon index.js --site=intranet --port=80",
    "api-operations": "nodemon index.js --site=api-operations",
    "api-partners": "nodemon index.js --site=api-partners",
    "api-sales": "nodemon index.js --site=api-sales",
    "api-shopify": "nodemon index.js --site=api-shopify",
    "api-timeclock": "nodemon index.js --site=api-timeclock",
    "api-user": "nodemon index.js --site=api-user",
    "start-b2b": "nodemon index.js --site=b2b --port=8081",
    "start-b2b-server": "nodemon index.js --site=b2b-server"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "http-proxy-rules": "^1.1.3",
    "nodemon": "^3.0.1"
  },
  "nodemonConfig": {
    "delay": 1500,
    "ignore": [
      "**/node_modules/*"
    ]
  }
}
