{
  "name": "local-proxy",
  "type": "module",
  "version": "2.1.0",
  "scripts": {
    "api-b2b": "nodemon index.js --site=b2b-api",
    "api-chums": "nodemon index.js --site=api-chums",
    "api-images": "nodemon index.js --site=api-images",
    "api-operations": "nodemon index.js --site=api-operations",
    "api-partners": "nodemon index.js --site=api-partners",
    "api-payroll": "nodemon index.js --site=api-payroll",
    "api-sage": "nodemon index.js --site=api-sage",
    "api-sales": "nodemon index.js --site=api-sales",
    "api-shopify": "nodemon index.js --site=api-shopify",
    "api-timeclock": "nodemon index.js --site=api-timeclock",
    "api-user": "nodemon index.js --site=api-user",
    "b2b": "nodemon index.js --site=b2b --port=80",
    "b2b:local": "nodemon index.js --site=b2b --port=80 --local=b2b-api",
    "b2b-api": "nodemon index.js --site=b2b-api",
    "b2b-server": "nodemon index.js --site=b2b-server",
    "intranet": "nodemon index.js --site=intranet --port=8081",
    "inventory-entry": "nodemon index.js --site=inventory-entry --port=8081",
    "intranet-80": "nodemon index.js --site=intranet --port=80"
  },
  "dependencies": {
    "@tsconfig/node22": "^22.0.0",
    "body-parser": "^2.2.0",
    "command-line-args": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.4",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-http-proxy": "^2.1.1",
    "http-proxy": "^1.18.1",
    "http-proxy-rules": "^1.1.3",
    "nodemon": "^3.0.1"
  },
  "nodemonConfig": {
    "delay": 1500,
    "ignore": [
      "**/node_modules/*"
    ]
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express-http-proxy": "^1.6.6",
    "@types/http-proxy": "^1.17.15",
    "@types/node": "^24.0.14",
    "typescript": "^5.6.2"
  }
}
